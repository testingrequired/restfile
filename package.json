{
  "name": "@testingrequired/restfile",
  "version": "1.0.0",
  "description": "REST request collections in a deterministic human readable/writable file format.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "restfile": "dist/cli.js"
  },
  "scripts": {
    "tsup": "tsup",
    "start": "node dist/index.js",
    "dev": "npm run build -- --watch --onSuccess \"npm run start\"",
    "build": "tsup src/index.ts src/cli.ts --dts --sourcemap --format esm,cjs",
    "precli": "npm run build",
    "cli": "node dist/cli.js",
    "test": "jest",
    "coverage": "npm test -- --coverage --collectCoverageFrom=\"./src/**\""
  },
  "author": "Kylee Tilley",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^17.0.21",
    "@types/node-fetch": "^2.6.1",
    "expect": "^27.5.1",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "tsup": "^5.11.13",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "http-z": "^6.1.2",
    "js-yaml": "^4.1.0",
    "node-fetch": "^2.6.7",
    "yargs": "^17.3.1"
  }
}
